{
  "family": "%LABEL%",
  "taskRoleArn": "arn:aws:iam::712852996757:role/telegraf-role",
  "containerDefinitions": [
    {
      "image": "712852996757.dkr.ecr.us-east-1.amazonaws.com/telegraf-deployment:latest",
      "name": "%LABEL%-container",
      "memoryReservation": 256,
      "cpu": 0,
      "portMappings": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "%LABEL%",
          "awslogs-group": "/ecs/telegraf-deployment"
        }
      },
      "environment": [
        {
          "name": "API_KEY",
          "value": "%API_KEY%"
        },
        {
          "name": "INFLUXDB_URL",
          "value": "http://testinflux.kontakt.io"
        },
        {
          "name": "INFLUXDB_USERNAME",
          "value": "%INFLUXDB_USERNAME%"
        },
        {
          "name": "INFLUXDB_PASSWORD",
          "value": "%INFLUXDB_PASSWORD%"
        },
        {
          "name": "API_URL",
          "value": "https://testapi.kontakt.io"
        },
        {
          "name": "MQTT_URL",
          "value": "ssl://testrtls.kontakt.io:8083"
        },
        {
          "name": "VENUE_ID",
          "value": "%VENUE_ID%"
        }
      ]
    }
  ]
}
