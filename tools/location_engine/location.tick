var period_time = 10s
var every_time = 5s

var database String
var retention_policy = 'stream_rp'

stream
    |from()
        .measurement('telemetry')
    |window()
        .period(period_time)
        .every(every_time)
        .align()
    @locationEngine()
    |influxDBOut()
        .database(database)
        .retentionPolicy(retention_policy)
        .measurement('locations')